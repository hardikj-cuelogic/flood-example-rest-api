# testing
source 'https://rubygems.org'

gem 'rails', '3.2.13'

gem 'pg'
gem 'jquery-rails'
gem 'twitter-bootstrap-rails'
gem 'sextant'
gem 'rails_12factor', group: :production

group :assets do
  gem 'sass-rails',   '~> 3.2.3'
  gem 'coffee-rails', '~> 3.2.1'
  gem 'uglifier', '>= 1.0.3'
end

group :development, :test do
  gem 'pry-rails'
  gem 'pry-nav'
  gem 'pry-debugger'
  gem 'better_errors'
  gem 'binding_of_caller'
end
